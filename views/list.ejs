<%- include('header'); -%>

  <div class="box" id="heading">
    <h1><%= listTitle %> List</h1>
  </div>
  <div class="box">
    <% for (let i = 0; i < tasks.length; i++) { %>
    <div class="item">
      <input type="checkbox">
      <p><%= tasks[i] %></p>
    </div>
    <% } %>


  <form action="/" method="post" class="item">
    <input type="text" name="newTask" placeholder="Enter a new task..." autocomplete="off">
    <button type="submit" name="addButton" value=<%= listTitle %>>+</button>
  </form>
  </div>
<%- include('footer'); -%>
